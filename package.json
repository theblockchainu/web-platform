{
  "name": "peerbuds2.0",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-blockchainu": "ng build BlockchainU --prod",
    "build:client-and-server-bundles": "ng build --prod && ng run peerbuds2.0:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "build-low-mem:ssr": "npm run build-low-mem:client-and-server-bundles && npm run webpack-low-mem:server",
    "build-low-mem:client-and-server-bundles": "npm run build-low-mem && npm run build-low-mem-server",
    "build-low-mem": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "build-low-mem-server": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng run peerbuds2.0:server",
    "webpack-low-mem:server": "node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js --config webpack.server.config.js --progress --colors",
    "build-blockchainu-low-mem": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build BlockchainU --prod"
  },
  "private": true,
